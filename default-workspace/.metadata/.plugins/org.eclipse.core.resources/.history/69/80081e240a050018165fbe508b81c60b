import java.util.ArrayList; 
import java.util.List;

public class Arraylist {

	public static void main(String[] args) {
		
		//Generics gives a type inside <> diamond operator
		List<Interger> numbers = new ArrayList<>();
		
		numbers.add(1);
		numbers.add(2);
		numbers.add(3);
		numbers.add(4);
		numbers.add(5);
		
		//Enhanced for loop aka for: each loop
		for(Interger number : numbers) {
			if(number % 2 ==0) {
			System.out.println(numbers);
			}
		}

	}

}

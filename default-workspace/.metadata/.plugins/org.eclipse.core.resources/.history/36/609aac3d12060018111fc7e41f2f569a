package shoppingcart;

public class Item {

	String name;
	int price;
	int quantity;

	public Item(String name, int i, int quantity) {
		this.name= name;
		this.price= i;
		this.quantity= quantity;
	}
	public Item() {
		
	}
	public int getPrice() {
		
		return price;
	}

	public String getName() {
		
		return name;
	}
}


